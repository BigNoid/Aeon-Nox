<window id="0">
  <defaultcontrol always="true">9000</defaultcontrol>
  <onload condition="Window.Previous(startup)+Skin.HasSetting(customstartupfocus)">Control.Move(9000,$INFO[Skin.String(CustomFocusOffset)])</onload>
  <onload condition="Window.Previous(startup)+[Skin.HasSetting(HomeRandomMovies)|Skin.HasSetting(HomeRandomTV)|Skin.HasSetting(HomeRandomMusic)|Skin.HasSetting(HomeRandomAddons)|Skin.HasSetting(HomeRandomAlbums)|Skin.HasSetting(HomeRandomPoster)]">XBMC.RunScript(script.randomitems,limit=8&amp;unplayed=True)</onload>
  <onload>ClearProperty(DontRunTVShowScripts,Home)</onload>
  <onload condition="!Skin.HasSetting(NoWidgetAlarm) + Control.isVisible(8998)">AlarmClock(WidgetMove,Control.Move(8000,1),00:07,silent,loop)</onload>
  <onload condition="!Skin.HasSetting(NoWidgetAlarmTwo) + Control.isVisible(8997)">AlarmClock(WidgetMoveTwo,Control.Move(8003,1),00:04,silent,loop)</onload>
  <onload condition="Skin.HasSetting(HomeMenuVideoPlaylist1) + [Skin.HasSetting(HomeLatestVPL1) | Skin.HasSetting(HomeLatestPosterVPL1)] + System.HasAddon(script.randomandlastitems)">XBMC.RunScript(script.randomandlastitems,playlist=$INFO[Skin.String(HomeMenuVideoPlaylist1_Path)],unwatched=$INFO[Skin.String(IncludeWatchedCVPL)],resume=False,limit=10,method=Last,menu=VPL1)</onload>
  <onload condition="Skin.HasSetting(HomeMenuVideoPlaylist2) + [Skin.HasSetting(HomeLatestVPL2) | Skin.HasSetting(HomeLatestPosterVPL2)] + System.HasAddon(script.randomandlastitems)">XBMC.RunScript(script.randomandlastitems,playlist=$INFO[Skin.String(HomeMenuVideoPlaylist2_Path)],unwatched=$INFO[Skin.String(IncludeWatchedCVPL)],resume=False,limit=10,method=Last,menu=VPL2)</onload>
  <onload condition="Skin.HasSetting(HomeMenuVideoPlaylist3) + [Skin.HasSetting(HomeLatestVPL3) | Skin.HasSetting(HomeLatestPosterVPL3)] + System.HasAddon(script.randomandlastitems)">XBMC.RunScript(script.randomandlastitems,playlist=$INFO[Skin.String(HomeMenuVideoPlaylist3_Path)],unwatched=$INFO[Skin.String(IncludeWatchedCVPL)],resume=False,limit=10,method=Last,menu=VPL3)</onload>
  <onload condition="IsEmpty(Skin.String(IncludeWatchedCVPL))">Skin.SetString(IncludeWatchedCVPL,False)</onload>
  <onload condition="IsEmpty(Skin.String(diffuse))">Skin.SetString(diffuse,FF6db9e5)</onload>
  <onload condition="IsEmpty(Skin.String(diffuse_bar))">Skin.SetString(diffuse_bar,FFFFFFFF)</onload>
  <onload condition="IsEmpty(Skin.String(diffuse_home))">Skin.SetString(diffuse_home,FFFFFFFF)</onload>
