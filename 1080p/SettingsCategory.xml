<window id="12">
  <defaultcontrol>3</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control type="group">
      <include>CommonSettingsBackground</include>
    </control>
    <include>CommonMediaPlayingBackground</include>
    <control type="group">
      <animation type="WindowOpen" reversible="false" condition="!Window.Previous(10004)">
        <effect type="zoom" start="150" end="100" center="960,540" time="900" tween="cubic" easing="out" />
        <effect type="fade" start="0" end="100" time="900" />
      </animation>
      <animation type="WindowClose" reversible="false" condition="!Window.Next(10004)">
        <effect type="fade" start="100" end="0" time="300" />
        <effect type="zoom" start="100" end="130" center="960,540" time="300" tween="cubic" easing="in" />
      </animation>
      <include>Furniture_SettingsBackPanel</include>
      <control type="group">
        <include>Animation_CommonFade</include>
        <include>Furniture_SettingsLogo</include>
        <control type="grouplist" id="3">
          <description>button area</description>
          <posx>63</posx>
          <posy>180</posy>
          <width>326</width>
          <height>768</height>
          <itemgap>0</itemgap>
          <onleft>60</onleft>
          <onright>5</onright>
          <onup>3</onup>
          <ondown>3</ondown>
        </control>
        <control type="label">
          <description>header label</description>
          <posx>358</posx>
          <posy>120</posy>
          <width>1491</width>
          <textcolor>white</textcolor>
          <font>Font_ShowcaseMainLabel2</font>
          <include>Animation_CommonFade</include>
          <label>$INFO[Control.GetLabel(2)] - $LOCALIZE[5]</label>
          <align>center</align>
          <aligny>center</aligny>
        </control>
        <control type="grouplist" id="5">
          <description>control area</description>
          <posx>387</posx>
          <posy>180</posy>
          <width>1404</width>
          <height>741</height>
          <itemgap>0</itemgap>
          <pagecontrol>60</pagecontrol>
          <onleft>3</onleft>
          <onright>60</onright>
          <onup>5</onup>
          <ondown>5</ondown>
        </control>
        <control type="scrollbar" id="60">
          <posx>1815</posx>
          <posy>180</posy>
          <width>9</width>
          <height>795</height>
          <showonepage>false</showonepage>
          <include>CommonVerticalScrollbar</include>
          <onleft>5</onleft>
          <onright>3</onright>
        </control>
        <include>Furniture_SettingsFrontPanel</include>
      </control>
      <control type="button" id="10">
        <description>Default Category Button</description>
        <posx>0</posx>
        <posy>4</posy>
        <width>324</width>
        <height>96</height>
        <texturefocus>views/listpanel_selectbar.png</texturefocus>
        <texturenofocus>-</texturenofocus>
        <font>Font_Bold20_Caps</font>
        <textcolor>FFf5f5f5</textcolor>
        <focusedcolor>FF262728</focusedcolor>
        <selectedcolor>FF262728</selectedcolor>
        <textoffsety>18</textoffsety>
        <textoffsetx>30</textoffsetx>
        <aligny>center</aligny>
        <pulseonselect>no</pulseonselect>
      </control>
      <control type="button" id="7">
        <description>Default Button</description>
        <include>Default_SettingButton</include>
      </control>
      <control type="radiobutton" id="8">
        <description>Default RadioButton</description>
        <include>Default_SettingRadioButton</include>
      </control>
      <control type="spincontrolex" id="9">
        <description>Default SpinControlex</description>
        <include>Default_SettingButton</include>
        <textureup>settings/spin-up.png</textureup>
        <texturedown>settings/spin-down.png</texturedown>
        <textureupfocus>settings/spin-up-focus.png</textureupfocus>
        <texturedownfocus>settings/spin-down-focus.png</texturedownfocus>
        <disabledcolor>60d0d0d0</disabledcolor>
        <aligny>center</aligny>
        <spinposx>-2</spinposx>
        <spinwidth>40</spinwidth>
        <spinheight>65</spinheight>
        <textoffsetx>75</textoffsetx>
        <reverse>yes</reverse>
      </control>
      <control type="image" id="11">
        <description>separator image</description>
        <height>0</height>
        <texture>settings/settings_categoryseparator.png</texture>
        <animation effect="slide" start="0,0" end="0,-3" time="0" condition="true">Conditional</animation>
      </control>
      <control type="label" id="2">
        <description>Fake Label so we can pass it value down to the one below</description>
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <font>-</font>
        <visible>false</visible>
      </control>
    </control>
    <control type="group">
      <description>Breadcrumbs</description>
      <visible>Skin.HasSetting(breadcrumb)</visible>
      <include>Animation_HideBreadCrumbs</include>
      <include>Breadcrumbs_BackBar</include>
      <control type="group">
        <control type="grouplist">
          <posx>22</posx>
          <posy>0</posy>
          <width>750</width>
          <align>left</align>
          <visible>
          </visible>
          <itemgap>0</itemgap>
          <orientation>horizontal</orientation>
          <control type="image">
            <description>Items Icon</description>
            <width>54</width>
            <height>34</height>
            <texture>common/settings_logo.png</texture>
            <include>BreadcrumbsIcon</include>
            <animation effect="slide" start="0,0" end="-9,2" time="0" condition="true">Conditional</animation>
          </control>
          <control type="label">
            <include>BreadcrumbsLabel</include>
            <label>$LOCALIZE[5]</label>
          </control>
          <control type="label">
            <include>BreadcrumbsLabel</include>
            <label>$INFO[Control.GetLabel(2), / ]</label>
          </control>
        </control>
      </control>
      <include>Breadcrumbs_Furniture</include>
    </control>
    <include condition="!Skin.HasSetting(GlobalNowPlaying)|Skin.HasSetting(GlobalRSS)">Furniture_NowPlayingWidget</include>
    <include condition="Skin.HasSetting(globaltime)">Furniture_Clock</include>
  </controls>
</window>