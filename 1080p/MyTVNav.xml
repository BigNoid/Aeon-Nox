<window id="25">
  <defaultcontrol always="true">50</defaultcontrol>
  <onload condition="System.HasAddon(script.tv.show.next.aired) + IsEmpty(Window(Home).Property(DontRunTVShowScripts)) + !Skin.HasSetting(NextAiredInfo)">RunScript(script.tv.show.next.aired,backend=True)</onload>
  <allowoverlay>no</allowoverlay>
  <views>50,51,52,55,57,500,501,503,504,505,508,509,510,511,512,514,516,550,551</views>
  <controls>
    <control type="group">
      <description>Fanart</description>
      <visible>!Control.IsVisible(516)</visible>
      <control type="multiimage" id="8861">
        <width>0</width>
        <height>0</height>
        <imagepath>$VAR[VideoExtraFanartVar]</imagepath>
        <visible>!substring(ListItem.Path,plugin://)</visible>
        <visible>[Container.Content(movies) + Skin.HasSetting(extrafanartmovies)] | [Container.Content(tvshows) + Skin.HasSetting(extrafanartseries)] | [Container.Content(seasons) + Skin.HasSetting(extrafanartseasons)] | [Container.Content(episodes) + Skin.HasSetting(extrafanartepisodes)]</visible>
      </control>
      <control type="image" id="8861">
        <width>0</width>
        <height>0</height>
        <texture fallback="empty.png">$VAR[VideoExtraFanartVar]</texture>
        <visible>Skin.HasSetting(SeasonFanart) + [Container.Content(seasons) | Container.Content(episodes)]</visible>
      </control>
      <control type="multiimage">
        <fadetime>FanartCrossfadeTime</fadetime>
        <animation effect="zoom" start="110" end="130" center="auto" time="15000" tween="linear" pulse="true" condition="Skin.HasSetting(animatedbg)">Conditional</animation>
        <animation effect="slide" start="-15,-15" end="15,15" time="7500" tween="linear" pulse="true" condition="Skin.HasSetting(animatedbg)">Conditional</animation>
        <imagepath background="true" fallback="special://skin/backgrounds/videos.jpg">$VAR[VideoFanartVar]</imagepath>
        <animation effect="fade" start="100" end="50" time="800">Hidden</animation>
        <visible>!Window.IsActive(movieinformation)</visible>
      </control>
    </control>
    <!-- view id = 516-->
    <include condition="!Skin.HasSetting(DisableBigFanVL)">BigFan</include>
    <include>VideoBackground</include>
    <include condition="!Skin.HasSetting(DisableBigFanVL)">BigFanFurniture</include>
    <control type="image" id="197">
      <posx>0</posx>
      <posy>0</posy>
      <width>0</width>
      <height>0</height>
      <texture>-</texture>
      <visible>[!Container.OnNext + !Container.OnPrevious + Skin.HasSetting(infopaneauto) + Container.Content(movies)] | [!Skin.HasSetting(infopaneauto) + Container.Content(movies)]</visible>
      <animation effect="fade" start="0" end="100" time="150" delay="1000" condition="Skin.HasSetting(1sec)">Visible</animation>
      <animation effect="fade" start="0" end="100" time="150" delay="2000" condition="Skin.HasSetting(2sec)">Visible</animation>
      <animation effect="fade" start="0" end="100" time="150" delay="3000" condition="Skin.HasSetting(3sec)">Visible</animation>
      <animation effect="fade" start="0" end="100" time="150" delay="4000" condition="Skin.HasSetting(4sec)">Visible</animation>
      <animation effect="fade" start="0" end="100" time="150" delay="5000" condition="Skin.HasSetting(5sec)">Visible</animation>
    </control>
    <control type="image" id="198">
      <posx>0</posx>
      <posy>0</posy>
      <width>0</width>
      <height>0</height>
      <texture>-</texture>
      <visible>[!Skin.HasSetting(infopaneautoep) + Container.Content(episodes)] | [!Skin.HasSetting(infopaneautotv) + [Container.Content(tvshows) | Container.Content(seasons)]] | [!Container.OnNext + !Container.OnPrevious + Skin.HasSetting(infopaneautotv) + Container.Content(tvshows)] | [!Container.OnNext + !Container.OnPrevious + Skin.HasSetting(infopaneautoep) + Container.Content(episodes)]</visible>
      <animation effect="fade" start="0" end="100" time="150" delay="1000" condition="Skin.HasSetting(1sec)">Visible</animation>
      <animation effect="fade" start="0" end="100" time="150" delay="2000" condition="Skin.HasSetting(2sec)">Visible</animation>
      <animation effect="fade" start="0" end="100" time="150" delay="3000" condition="Skin.HasSetting(3sec)">Visible</animation>
      <animation effect="fade" start="0" end="100" time="150" delay="4000" condition="Skin.HasSetting(4sec)">Visible</animation>
      <animation effect="fade" start="0" end="100" time="150" delay="5000" condition="Skin.HasSetting(5sec)">Visible</animation>
    </control>
    <include>Furniture_Showcase</include>
    <include condition="!Skin.HasSetting(DisableInfoWallVL) | !Skin.HasSetting(DisableWallVL) | !Skin.HasSetting(DisableBannerWallVL) | !Skin.HasSetting(DisableThumbnailVL)">WallPanel</include>
    <control type="group">
      <visible>Control.IsVisible(508) | Control.IsVisible(503) | Control.IsVisible(509) | Control.IsVisible(504) | Control.IsVisible(501)</visible>
      <include condition="!Skin.HasSetting(DisableBannersVL) | !Skin.HasSetting(DisableLandscapeVL) | !Skin.HasSetting(DisableShowCaseVL) | !Skin.HasSetting(DisableDVDCaseVL) | !Skin.HasSetting(DisablePostersVL)">ShowcaseInfoPaneMovies</include>
      <animation effect="slide" end="0,45" time="400" easing="inout" tween="cubic" condition="Control.IsVisible(503) | Control.IsVisible(508)">Conditional</animation>
    </control>
    <control type="group">
      <!-- view id = 50 -->
      <include>CommonRootView</include>
      <!--view id = 551-->
      <include condition="!Skin.HasSetting(DisableTriPanelVL)">TriPanel</include>
      <!--view id = 551-->
      <include condition="!Skin.HasSetting(DisableThumbnailVL)">ThumbnailView</include>
      <!-- view id = 500 -->
      <include condition="!Skin.HasSetting(DisableWallVL)">Viewtype_Wall</include>
      <!-- view id = 501 Showcase-->
      <include condition="!Skin.HasSetting(DisableShowCaseVL)">Showcase</include>
      <!-- view id = 508 Bannerview-->
      <include condition="!Skin.HasSetting(DisableBannersVL) + IsEmpty(Window(Home).Property(DontRunTVShowScripts))">Banners</include>
      <!-- view id = 503 Landscape-->!
      <include condition="!Skin.HasSetting(DisableLandscapeVL)">MediaListView3</include><!-- view id = 504 Showcase DVD--><include condition="!Skin.HasSetting(DisableDVDCaseVL)">ShowcaseDVD</include><!-- view id = 505 --><include condition="!Skin.HasSetting(DisableInfoListVL)">InfoList</include><!-- view id = 511 --><include condition="!Skin.HasSetting(DisableLowListVL)">LowList</include><!-- view id = 51 --><include condition="!Skin.HasSetting(DisableBannerWallVL) + IsEmpty(Window(Home).Property(DontRunTVShowScripts))">BannerWall</include><!-- view id = 550--><include condition="!Skin.HasSetting(DisablePanelVL)">Panel</include><!-- view id = 510 --><include condition="!Skin.HasSetting(DisablePostersVL)">PostersView</include><!-- view id = 57--><include condition="!Skin.HasSetting(DisableShiftVL)">ShiftView</include><!-- view id = 509--><include condition="!Skin.HasSetting(DisableShowCaseVL) + IsEmpty(Window(Home).Property(DontRunTVShowScripts))">ShowcaseQuadrate</include><!-- view id = 512--><include condition="!Skin.HasSetting(DisableInfoWallVL)">AddonWall</include><!--view id = 514--><include condition="!Skin.HasSetting(DisableEpisodeVL) + IsEmpty(Window(Home).Property(DontRunTVShowScripts))">Viewtype_Episode</include><!--view id = 55--><include condition="!Skin.HasSetting(DisableCarouselVL)">Carousel</include></control>
    <include condition="Skin.HasSetting(ScrollLabel)">ScrollOffsetLabel</include>
    <control type="group">
      <description>Invisible buttons for trailer</description>
      <control type="button" id="7000">
        <include>HiddenObject</include>
        <onfocus condition="[[Skin.HasSetting(showcaseinfoon) + Container.Content(movies) + IsEmpty(ListItem.Trailer)] | [Skin.HasSetting(showcaseinfoonep) + Container.Content(episodes)] | [Skin.HasSetting(showcaseinfoontv) + [Container.Content(tvshows) | Container.Content(seasons)]]] + [Control.IsVisible(501) | Control.IsVisible(504) | Control.IsVisible(503) | Control.IsVisible(508)]">SetFocus(9000)</onfocus>
        <onfocus condition="[Control.IsVisible(501) | Control.IsVisible(504) | Control.IsVisible(503)]">SetFocus(7001)</onfocus>
        <onup>9000</onup>
        <onleft condition="Control.IsVisible(501)">Control.Move(501,-1)</onleft>
        <onleft condition="Control.IsVisible(504)">Control.Move(504,-1)</onleft>
        <onleft condition="Control.IsVisible(503)">Control.Move(503,-1)</onleft>
        <onleft condition="Control.IsVisible(508)">Control.Move(508,-1)</onleft>
        <onright condition="Control.IsVisible(501)">Control.Move(501,1)</onright>
        <onright condition="Control.IsVisible(504)">Control.Move(504,1)</onright>
        <onright condition="Control.IsVisible(503)">Control.Move(503,1)</onright>
        <onright condition="Control.IsVisible(508)">Control.Move(508,1)</onright>
        <ondown>50</ondown>
        <onback>50</onback>
        <onclick>PlayMedia($ESCINFO[ListItem.FileNameAndPath])</onclick>
        <visible>true</visible>
      </control>
      <control type="button" id="7002">
        <include>HiddenObject</include>
        <onup>Control.Move(511,-1)</onup>
        <onleft>50</onleft>
        <onright condition="!Skin.HasSetting(kioskmode)">9000</onright>
        <ondown>Control.Move(511,1)</ondown>
        <onback>50</onback>
        <onclick>PlayMedia($ESCINFO[ListItem.FileNameAndPath])</onclick>
        <visible>Control.IsVisible(511)</visible>
      </control>
      <control type="button" id="7001">
        <include>HiddenObject</include>
        <onleft condition="Control.isVisible(50)">62</onleft>
        <onleft condition="Control.isVisible(550)">50</onleft>
        <onleft condition="!Control.isVisible(50) + !Control.IsVisible(550)">9000</onleft>
        <onright condition="Control.isVisible(50)">9000</onright>
        <onright condition="!Control.isVisible(50) + !Control.IsVisible(550)">50</onright>
        <onright condition="Control.IsVisible(550)">60</onright>
        <ondown>50</ondown>
        <onback>50</onback>
        <onup>9000</onup>
        <onclick condition="!Skin.HasSetting(fstrailer)">PlayMedia($ESCINFO[ListItem.Trailer],1)</onclick>
        <onclick condition="Skin.HasSetting(fstrailer)">PlayMedia($ESCINFO[ListItem.Trailer])</onclick>
        <onclick>SetFocus(50)</onclick>
        <font>-</font>
        <label>-</label>
      </control>
    </control>
    <control type="group">
      <visible>Control.IsVisible(508) | Control.IsVisible(503) | Control.IsVisible(509) | Control.IsVisible(504) | Control.IsVisible(501) | Control.IsVisible(51) | Control.IsVisible(551) | Control.IsVisible(500) | [Control.IsVisible(512) + !Skin.HasSetting(infowallflags)] | Control.IsVisible(516) | [Control.IsVisible(514) + !Skin.HasSetting(infowallflags)] | Control.IsVisible(55)</visible>
      <animation effect="slide" start="0" end="705" time="0" easing="inout" tween="cubic" condition="Control.IsVisible(514)">Conditional</animation>
      <include condition="!Skin.HasSetting(DisableThumbnailVL) | !Skin.HasSetting(DisableInfoWallVL) | !Skin.HasSetting(DisableStripshowVL) | !Skin.HasSetting(DisableShiftVL) | !Skin.HasSetting(DisableBannersVL) | !Skin.HasSetting(DisableLandscapeVL) | !Skin.HasSetting(DisableShowCaseVL) | !Skin.HasSetting(DisableDVDCaseVL) | !Skin.HasSetting(DisablePostersVL) | !Skin.HasSetting(DisableCarouselVL) | !Skin.HasSetting(DisableWallVL) | !Skin.HasSetting(DisableBannerWallVL)">FloorFlags</include>
    </control>
    <include>FloorLabels</include>
    <include condition="!Skin.HasSetting(DisablePostersVL) | !Skin.HasSetting(DisableShiftVL) |!Skin.HasSetting(DisableTriPanelVL)">Posters_Furniture</include>
    <include>Furniture_DarkGradient</include>
    <control type="group">
      <description>Breadcrumbs</description>
      <include>Animation_HideBreadCrumbs</include>
      <include>Breadcrumbs_BackBar</include>
      <control type="group">
        <control type="grouplist">
          <posx>22</posx>
          <posy>0</posy>
          <width>750</width>
          <align>left</align>
          <visible>
          </visible>
          <itemgap>0</itemgap>
          <orientation>horizontal</orientation>
          <visible>!Skin.HasSetting(GlobalRSS)</visible>
          <visible>![Player.HasMedia + !Skin.HasSetting(GlobalNowPlaying) + IsEmpty(Window(videolibrary).Property(tvtunesIsAlive))]</visible>
          <control type="image">
            <description>Items Icon</description>
            <width>54</width>
            <height>38</height>
            <aspectratio>stretch</aspectratio>
            <texture>common/tv_icon.png</texture>
            <include>BreadcrumbsIcon</include>
            <animation effect="slide" start="0,0" end="-9,0" time="0" condition="true">Conditional</animation>
          </control>
          <control type="label">
            <include>BreadcrumbsLabel</include>
            <label>$VAR[BreadcrumbsVideoVar]</label>
          </control>
          <control type="label">
            <include>BreadcrumbsLabel</include>
            <label>$INFO[Container.FolderName, / ]</label>
            <include>BreadcrumbsLabel</include>
          </control>
        </control>
      </control>
      <include>Breadcrumbs_Furniture</include>
    </control>
    <include condition="!Skin.HasSetting(GlobalNowPlaying) | Skin.HasSetting(GlobalRSS)">Furniture_NowPlayingWidget</include>
    
    <include condition="!Skin.HasSetting(kioskmode)">Media_Menu</include>
  </controls>
</window>