      <!-- view id = 504 Showcase DVD-->
      <include condition="!Skin.HasSetting(DisableDVDCaseVL)">ShowcaseDVD</include>
      <!-- view id = 505 -->
      <include condition="!Skin.HasSetting(DisableInfoListVL)">InfoList</include>
      <!-- view id = 511 -->
