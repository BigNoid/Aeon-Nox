        <texture background="true">$VAR[VideoFanartVar]</texture>
        <visible>[!Skin.HasSetting(VideoLibraryMovieFanart)+Container.Content(movies)]|[!Skin.HasSetting(VideoLibraryTVFanart)+[Container.Content(TVShows) | Container.Content(Seasons) | Container.Content(Episodes)]]|[!Skin.HasSetting(VideoLibraryMusicVideoFanart)+[Container.Content(MusicVideos) | Container.Content(Artists) | Container.Content(Albums)]]|Container.Content(addons)|Container.Content(files)</visible>
