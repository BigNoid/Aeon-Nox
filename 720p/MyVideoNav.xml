        <randomize>false</randomize>
        <include>CommonFanartVars</include>
        <imagepath background="true">$VAR[VideoExtraFanartVar]</imagepath>
        <visible>[!Skin.HasSetting(VideoLibraryMovieFanart)+Container.Content(movies)]|[!Skin.HasSetting(VideoLibraryTVFanart)+[Container.Content(TVShows) | Container.Content(Seasons) | Container.Content(Episodes)]]|Container.Content(genres)</visible>
