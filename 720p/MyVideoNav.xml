<window id="25">
  <defaultcontrol always="true">50</defaultcontrol>
  <onload condition="Skin.HasSetting(tv_theme) + System.HasAddon(script.tvtunes) + !SubString(Window(10025).Property(CinemaExperienceRunning),True)">XBMC.RunScript(script.tvtunes,backend=True)</onload>
  <onload condition="System.HasAddon(script.tv.show.next.aired)">RunScript(script.tv.show.next.aired,backend=True)</onload>
  <allowoverlay>no</allowoverlay>
  <views>50,51,501,504,503,500,508,509,512,514,516,505,510,511,550,560,55,517</views>
