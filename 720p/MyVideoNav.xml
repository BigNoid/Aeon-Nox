    <control type="group">
      <visible>[Control.IsVisible(510)+!Skin.HasSetting(posterslide)]|Control.IsVisible(514)|Control.IsVisible(512)|Control.IsVisible(50)</visible>
      <include>VideoWindowPosters</include>
    </control>
    <include>ScrollOffsetLabel</include>
    <control type="group">
      <description>Invisible buttons for trailer</description>
      <control type="button" id="7000">
        <include>HiddenObject</include>
        <onfocus condition="[[Skin.HasSetting(showcaseinfoon)+Container.Content(movies)+IsEmpty(ListItem.Trailer)]|[Skin.HasSetting(showcaseinfoonep)+Container.Content(episodes)]|[Skin.HasSetting(showcaseinfoontv)+[Container.Content(tvshows)|Container.Content(seasons)]]]+[Control.IsVisible(501)|Control.IsVisible(504)|Control.IsVisible(503)|Control.IsVisible(508)|[Control.IsVisible(510)+Skin.HasSetting(posterslide)]]">SetFocus(9000)</onfocus>
        <onfocus condition="[Skin.HasSetting(showcaseinfoon)+Container.Content(movies)+!IsEmpty(ListItem.Trailer)]+[Control.IsVisible(501)|Control.IsVisible(504)|Control.IsVisible(503)|[Control.IsVisible(510)+Skin.HasSetting(posterslide)]]">SetFocus(7001)</onfocus>
        <onup condition="IsEmpty(ListItem.Trailer)">SetFocus(9000)</onup>
        <onup condition="!IsEmpty(ListItem.Trailer)">SetFocus(7001)</onup>
        <onleft condition="Control.IsVisible(501)">Control.Move(501,-1)</onleft>
        <onleft condition="Control.IsVisible(510)">Control.Move(510,-1)</onleft>
        <onleft condition="Control.IsVisible(504)">Control.Move(504,-1)</onleft>
        <onleft condition="Control.IsVisible(503)">Control.Move(503,-1)</onleft>
        <onleft condition="Control.IsVisible(508)">Control.Move(508,-1)</onleft>
        <onright condition="Control.IsVisible(501)">Control.Move(501,1)</onright>
        <onright condition="Control.IsVisible(510)">Control.Move(510,1)</onright>
        <onright condition="Control.IsVisible(504)">Control.Move(504,1)</onright>
        <onright condition="Control.IsVisible(503)">Control.Move(503,1)</onright>
        <onright condition="Control.IsVisible(508)">Control.Move(508,1)</onright>
        <ondown>50</ondown>
        <onback>50</onback>
        <onclick>PlayMedia($INFO[ListItem.FileNameAndPath])</onclick>
        <visible>true</visible>
        <!--<visible>[!Skin.HasSetting(showcaseinfoon)+Container.Content(movies)]|[!Skin.HasSetting(showcaseinfoonep)+Container.Content(episodes)]|[!Skin.HasSetting(showcaseinfoontv)+Container.Content(tvshows)]</visible>-->
      </control>
      <control type="button" id="7002">
        <include>HiddenObject</include>
        <onup>Control.Move(511,-1)</onup>
        <onleft>50</onleft>
        <onright>9000</onright>
        <ondown>Control.Move(511,1)</ondown>
        <onback>50</onback>
        <onclick>PlayMedia($INFO[ListItem.FileNameAndPath])</onclick>
        <visible>Control.IsVisible(511)+!Container.Content(seasons)+!Container.Content(tvshows)</visible>
      </control>
      <control type="button" id="7001">
        <include>HiddenObject</include>
        <onleft condition="Control.isVisible(50)|Control.isVisible(550)">50</onleft>
        <onleft condition="!Control.isVisible(50)+!Control.IsVisible(550)">SetFocus(9000)</onleft>
        <onright condition="Control.isVisible(50)">9000</onright>
        <onright condition="!Control.isVisible(50)+!Control.IsVisible(550)">SetFocus(50)</onright>
        <onright condition="Control.IsVisible(550)">60</onright>
        <ondown>50</ondown>
        <onback>50</onback>
        <onup>9000</onup>
        <onclick condition="!Skin.HasSetting(fstrailer)">PlayMedia($INFO[ListItem.Trailer],1)</onclick>
        <onclick condition="Skin.HasSetting(fstrailer)">PlayMedia($INFO[ListItem.Trailer])</onclick>
        <onclick>SetFocus(50)</onclick>
        <font>-</font>
        <label>-</label>
        <visible>!IsEmpty(ListItem.Trailer) + !Player.HasVideo</visible>
      </control>
