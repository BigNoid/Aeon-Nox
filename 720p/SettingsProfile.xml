<window id="20">
  <defaultcontrol always="true">2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <include>CommonSettingsBackground</include>
    <include>CommonMediaPlayingBackground</include>
    <control type="group">
      <visible>![Window.isVisible(ProfileSettings)|Window.IsVisible(LockSettings)]</visible>
      <animation type="WindowOpen" reversible="false" condition="!Window.Previous(10004)">
        <effect type="zoom" start="150" end="100" center="640,360" time="300" tween="cubic" easing="out" />
        <effect type="fade" start="0" end="100" time="300" />
      </animation>
      <animation type="WindowClose" reversible="false" condition="!Window.Next(10004)">
        <effect type="fade" start="100" end="0" time="300" />
        <effect type="zoom" start="100" end="130" center="640,360" time="300" tween="cubic" easing="in" />
      </animation>
      <include>Furniture_SettingsBackPanel</include>
      <include>Furniture_SettingsLogo</include>
      <control type="list" id="2">
        <posx>42</posx>
        <posy>126</posy>
        <width>500</width>
        <height>378</height>
        <itemgap>0</itemgap>
        <onleft>9001</onleft>
        <onright>9001</onright>
        <onup>2</onup>
        <ondown>2</ondown>
        <pagecontrol>-</pagecontrol>
        <scrolltime>300</scrolltime>
        <itemlayout height="54" width="380">
          <control type="label">
            <posx>20</posx>
            <posy>0</posy>
            <width>204</width>
            <height>36</height>
            <font>Font_Bold20_Caps</font>
            <textcolor>FFf5f5f5</textcolor>
            <aligny>center</aligny>
            <label>$INFO[ListItem.Label]</label>
          </control>
          <control type="label">
            <posx>20</posx>
            <posy>27</posy>
            <width>204</width>
            <height>18</height>
            <font>Font_Info_Caps</font>
            <textcolor>FFf5f5f5</textcolor>
            <align>left</align>
            <aligny>center</aligny>
            <label>[UPPERCASE]$INFO[ListItem.Label2][/UPPERCASE]</label>
          </control>
        </itemlayout>
        <focusedlayout height="54" width="380">
          <control type="image">
            <posx>0</posx>
            <posy>-4</posy>
            <width>216</width>
            <height>64</height>
            <texture>views/listpanel_selectbar.png</texture>
            <animation effect="fade" start="100" end="50" time="50" condition="!Control.HasFocus(2)">Conditional</animation>
          </control>
          <control type="label">
            <posx>20</posx>
            <posy>0</posy>
            <width>204</width>
            <height>36</height>
            <font>Font_Bold20_Caps</font>
            <textcolor>ff2e2e2e</textcolor>
            <aligny>center</aligny>
            <label>$INFO[ListItem.Label]</label>
          </control>
          <control type="label">
            <posx>20</posx>
            <posy>27</posy>
            <width>204</width>
            <height>18</height>
            <font>Font_Info_Caps</font>
            <textcolor>FF262728</textcolor>
            <align>left</align>
            <aligny>center</aligny>
            <label>[UPPERCASE]$INFO[ListItem.Label2][/UPPERCASE]</label>
          </control>
        </focusedlayout>
      </control>

      <control type="grouplist" id="9001">
        <posx>258</posx>
        <posy>130</posy>
        <width>936</width>
        <height>481</height>
        <itemgap>-1</itemgap>
        <pagecontrol>60</pagecontrol>
        <onleft>2</onleft>
        <onright>60</onright>
        <onup>9001</onup>
        <ondown>9001</ondown>
        <control type="radiobutton" id="4">
          <label>20096</label>
          <include>Default_SettingRadioButton</include>
        </control>
      </control>
      <control type="label">
        <description>header label</description>
        <posx>239</posx>
        <posy>80</posy>
        <width>994</width>
        <font>Font_ShowcaseMainLabel2</font>
        <label>$LOCALIZE[10034]</label>
        <align>center</align>
        <aligny>center</aligny>
        <textcolor>white</textcolor>
        <shadowcolor>black</shadowcolor>
      </control>
      <include>Furniture_SettingsFrontPanel</include>
    </control>
    <control type="group">
      <description>Breadcrumbs</description>
      <visible>Skin.HasSetting(breadcrumb)</visible>
      <include>Animation_HideBreadCrumbs</include>
      <include>Breadcrumbs_BackBar</include>
      <control type="group">
        <control type="grouplist">
          <posx>15</posx>
          <posy>0</posy>
          <width>500</width>
          <align>left</align>
          <visible></visible>
          <itemgap>0</itemgap>
          <orientation>horizontal</orientation>
          <control type="image">
            <description>Items Icon</description>
            <width>36</width>
            <height>23</height>
            <texture>common/settings_logo.png</texture>
			<include>BreadcrumbsIcon</include>
            <animation effect="slide" start="0,0" end="-6,1" time="0" condition="true">Conditional</animation>
          </control>
          <control type="label">
            <include>BreadcrumbsLabel</include>
            <label>$LOCALIZE[5]</label>
          </control>
          <control type="label">
            <include>BreadcrumbsLabel</include>
            <label> / $LOCALIZE[13200]</label>
          </control>
        </control>
      </control>
      <include>Breadcrumbs_Furniture</include>
    </control>

    <include condition="!Skin.HasSetting(GlobalNowPlaying)">Furniture_NowPlayingWidget</include>
<include condition="!Skin.HasSetting(breadcrumb) + Skin.HasSetting(GlobalRSS)">GlobalRSS</include>
    <include condition="Skin.HasSetting(globaltime)">Furniture_Clock</include>
  </controls>
</window>
