<includes>
  <include name="FloorRatingVars">
    <width>90</width>
    <height>60</height>
    <aspectratio>keep</aspectratio>
    <fadetime>IconCrossfadeTime2</fadetime>
    <colordiffuse>floor</colordiffuse>
  </include>
  <include name="FloorFlags">
