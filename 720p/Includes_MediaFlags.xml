      <visible>[Control.IsVisible(51)|Control.IsVisible(500)|Control.IsVisible(501)|Control.IsVisible(503)|Control.IsVisible(504)|Control.IsVisible(508)|Control.IsVisible(510)]+!Window.IsActive(1)</visible>
      <include>Animation_HideMultiplex</include>
      <control type="group">
        <posx>10</posx>
        <posy>658</posy>
        <visible>Skin.HasSetting(sourceflags)</visible>
        <animation effect="slide" start="70" end="70" condition="Skin.HasSetting(nostudioratingflags)">Conditional</animation>
        <control type="image">
          <include>AudioFlagsVars</include>
          <texture>flags/source/sdtv.png</texture>
          <visible>SubString(ListItem.filename,PDTV) | SubString(ListItem.filename,sdtv)</visible>
        </control>
        <control type="image">
          <include>AudioFlagsVars</include>
          <texture>flags/source/hdtv.png</texture>
          <visible>SubString(ListItem.Filename,hdtv)</visible>
        </control>
        <control type="image">
          <include>AudioFlagsVars</include>
          <texture>flags/source/hddvd.png</texture>
          <visible>SubString(ListItem.Filename,hddvd) | SubString(ListItem.Filename,hd-dvd)</visible>
        </control>
        <control type="image">
          <include>AudioFlagsVars</include>
          <texture>flags/source/bluray.png</texture>
          <visible>SubString(ListItem.Filename,bluray) | SubString(ListItem.Filename,blu-ray) | SubString(ListItem.Filename,bdrip) | SubString(ListItem.Filename,brrip)</visible>
        </control>
        <control type="image">
          <include>AudioFlagsVars</include>
          <texture>flags/source/dvd.png</texture>
          <visible>[SubString(ListItem.Filename,dvd) + !SubString(ListItem.Filename,hddvd) + !SubString(ListItem.Filename,hd-dvd) + !SubString(ListItem.Filename,bluray)] | [SubString(ListItem.Filename,.iso,Right) + !SubString(ListItem.Filename,bluray)] | SubString(ListItem.Filename,.ifo,Right)</visible>
