<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="albums">
    <name>Top Rated Albums</name>
    <match>all</match>
    <rule field="rating" operator="greaterthan">
        <value>4</value>
    </rule>
    <limit>25</limit>
    <order direction="ascending">random</order>
</smartplaylist>
