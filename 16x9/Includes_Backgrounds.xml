					<width>100%</width>
					<height>1266</height>
					<texture colordiffuse="$VAR[ThemeLabelColor]">floor.png</texture>
				</control>
			</control>
			<control type="group">
				<visible>Window.IsActive(settings) | Window.IsActive(SettingsCategory.xml) | Window.IsActive(1140) | Window.IsActive(1117) | Window.IsActive(systeminfo) | Window.IsActive(profiles) | Window.IsActive(weather)</visible>
