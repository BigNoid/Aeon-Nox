<?xml version="1.0" encoding="UTF-8"?>
<window>
	<coordinates>
		<left>560</left>
		<top>30</top>
	</coordinates>
	<include>Animation_DialogPopup</include>
	<controls>
		<control type="group">
			<visible>!Window.IsActive(fullscreenvideo) + !Window.IsActive(visualisation) + !Player.HasVideo</visible>
			<control type="image">
				<description>background image</description>
				<left>0</left>
				<top>0</top>
				<width>800</width>
				<height>166</height>
				<texture border="40">dialogs/default/bg.png</texture>
			</control>
			<control type="image">
				<left>30</left>
				<top>32</top>
				<width>740</width>
				<height>102</height>
				<texture border="5">dialogs/default/inner.png</texture>
			</control>
			<control type="progress" id="32">
				<description>progress control</description>
				<left>32</left>
				<top>33</top>
				<width>738</width>
				<height>88</height>
				<texturebg border="2">osd/texturebg2.png</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture colordiffuse="$VAR[ThemeLabelColor]" border="2">osd/midtexture.png</midtexture>
				<righttexture>-</righttexture>
				<overlaytexture>-</overlaytexture>
				<colordiffuse>44FFFFFF</colordiffuse>
			</control>
			<control type="label">
				<description>Header Label</description>
				<left>0</left>
				<top>32</top>
				<width>800</width>
				<height>50</height>
				<font>font13</font>
				<textcolor>grey</textcolor>
				<textoffsetx>50</textoffsetx>
				<align>left</align>
				<aligny>center</aligny>
				<label>$INFO[Control.GetLabel(30)]$INFO[Control.GetLabel(32),: ,%]</label>
			</control>
			<control type="label" id="31">
				<description>Title Label</description>
				<left>0</left>
				<top>82</top>
				<width>800</width>
				<height>50</height>
				<font>font13</font>
				<textcolor>white</textcolor>
				<align>left</align>
				<aligny>center</aligny>
				<textoffsetx>50</textoffsetx>
			</control>
			<control type="label" id="30">
				<description>Header Label</description>
				<include>HiddenObject</include>
			</control>
		</control>
	</controls>
</window>