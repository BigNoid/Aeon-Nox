				<control type="textbox">
					<left>4</left>
					<top>160</top>
					<width>1000</width>
					<height>480</height>
					<label>$INFO[VideoPlayer.Plot]</label>
					<pagecontrol>60</pagecontrol>
					<autoscroll delay="15000" time="AutoScrollTime" repeat="10000">Skin.HasSetting(AutoScroll) + !Control.HasFocus(60)</autoscroll>
					<visible>!Control.IsVisible(50)</visible>
				</control>
				<control type="image">
					<!-- Logo background -->
					<left>1050</left>
					<top>-20</top>
					<width>250</width>
					<height>200</height>
					<texture>pvr/bg_channel_icon.png</texture>
					<align>center</align>
					<visible>false</visible>
				</control>
				<control type="image">
					<!-- Channel logo -->
					<left>1050</left>
					<top>150</top>
					<width>200</width>
					<height>200</height>
					<aspectratio>keep</aspectratio>
					<align>center</align>
					<texture>$INFO[VideoPlayer.Cover]</texture>
					<visible>IsEmpty(VideoPlayer.PlotOutline)</visible>
				</control>
				<control type="image">
					<!-- Show\Movie poster -->
					<left>1050</left>
					<top>160</top>
					<width>250</width>
					<height>440</height>
					<align>center</align>
					<aspectratio>keep</aspectratio>
					<texture>$INFO[VideoPlayer.PlotOutline]</texture>
					<visible>!IsEmpty(VideoPlayer.PlotOutline)</visible>
				</control>
			</control>
		</control>
	</controls>
</window>