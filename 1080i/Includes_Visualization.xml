<?xml version="1.0" encoding="utf-8"?>
<includes>
    <include name="MusicVizBG">
        <aspectratio align="center" aligny="top">scale</aspectratio>
        <fadetime>1000</fadetime>
        <include>FullscreenDimensions</include>
        <include>Animation_WindowFade200</include>
        <include>Animation_VisibleChange400</include>
        <animation effect="zoom" start="110" end="130" center="auto" time="12000" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
        <animation effect="slide" start="-15,-15" end="15,15" time="6000" tween="sine" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
        <timeperimage>10000</timeperimage>
    </include>
    <include name="cdwall-musicvisualisation">
        <control type="group" id="2355">
            <posx>-52</posx>
            <posy>-22</posy>
            <!--         <animation effect="zoom" start="110" end="130" center="auto" time="15000" pulse="true" condition="true">Conditional</animation>
        <animation effect="slide" start="-15,-15" end="15,15" time="7500" tween="sine" easing="inout" pulse="true" condition="true">Conditional</animation>
        <animation effect="rotatex" start="" end="15" time="0" condition="true">Conditional</animation> -->
            <visible>[Window.IsActive(visualisation) + StringCompare(Skin.String(MusicViz.MultiFanart),cd-wall)] | [Player.HasAudio + [Window.IsActive(home) | Window.IsActive(1158)] + Skin.HasSetting(homecdwall)] | Stringcompare(Control.GetLabel(4322),cdwall)</visible>
            <animation effect="zoom" start="110" end="130" center="auto" time="12000" pulse="true" condition="[![Window.IsActive(home) | Window.IsActive(1124)] + Skin.HasSetting(animation_bg)] | [[Window.IsActive(home) | Window.IsActive(1124)] + Skin.HasSetting(animatedbghome)]">Conditional</animation>
            <animation effect="slide" start="-15,-15" end="15,15" time="6000" tween="sine" easing="inout" pulse="true" condition="[![Window.IsActive(home) | Window.IsActive(1124)] + Skin.HasSetting(animation_bg)] | [[Window.IsActive(home) | Window.IsActive(1124)] + Skin.HasSetting(animatedbghome)]">Conditional</animation>
            <include>Animation_WindowFade200</include>
            <include>Animation_VisibleChange400</include>
			<control type="panel" id="9032">
				<posx>0</posx>
				<posy>0</posy>
				<width>1920</width>
				<height>1080</height>
				<movement>6</movement>
				<pagecontrol>60</pagecontrol>
				<scrolltime tween="cubic" easing="inout">600</scrolltime>
				<orientation>vertical</orientation>
				<preloaditems>0</preloaditems>
				<itemlayout height="192" width="192">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>192</width>
						<height>192</height>
						<aspectratio>scale</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</itemlayout>
				<focusedlayout height="192" width="192">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>192</width>
						<height>192</height>
						<aspectratio>scale</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</focusedlayout>
				<content>special://skin/extras/playlists/albumwall.xsp</content>
			</control>
        </control>
    </include>
    <include name="MovieWall">
        <control type="group" id="2355">
            <!--         <animation effect="zoom" start="110" end="130" center="auto" time="15000" pulse="true" condition="true">Conditional</animation>
        <animation effect="slide" start="-15,-15" end="15,15" time="7500" tween="sine" easing="inout" pulse="true" condition="true">Conditional</animation>
        <animation effect="rotatex" start="" end="15" time="0" condition="true">Conditional</animation> -->
            <visible>SubString(Control.GetLabel(4322),moviewall)</visible>
            <animation effect="zoom" start="110" end="130" center="auto" time="12000" pulse="true" condition="[![Window.IsActive(home) | Window.IsActive(1124)] + Skin.HasSetting(animation_bg)] | [[Window.IsActive(home) | Window.IsActive(1124)] + Skin.HasSetting(animatedbghome)]">Conditional</animation>
            <animation effect="slide" start="-15,-15" end="15,15" time="6000" tween="sine" easing="inout" pulse="true" condition="[![Window.IsActive(home) | Window.IsActive(1124)] + Skin.HasSetting(animation_bg)] | [[Window.IsActive(home) | Window.IsActive(1124)] + Skin.HasSetting(animatedbghome)]">Conditional</animation>
            <include>Animation_WindowFade200</include>
            <include>Animation_VisibleChange400</include>
			<control type="panel" id="9032">
				<posx>0</posx>
				<posy>0</posy>
				<width>2120</width>
				<height>1080</height>
				<movement>6</movement>
				<scrolltime tween="cubic" easing="inout">600</scrolltime>
				<orientation>vertical</orientation>
				<preloaditems>0</preloaditems>
				<itemlayout height="337" width="225">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
				        <width>225</width>
                        <height>337</height>
						<aspectratio>scale</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</itemlayout>
				<focusedlayout height="337" width="225">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
                        <width>225</width>
                        <height>337</height>
						<aspectratio>scale</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</focusedlayout>
				<content>$VAR[MovieWallPathVar]</content>
			</control>
        </control>
    </include>
</includes>