        <width>60</width>
        <height>98</height>
        <font>font13</font>
        <reverse>yes</reverse>
        <textcolor>white</textcolor>
        <disabledcolor>grey3</disabledcolor>
        <subtype>text</subtype>
        <textoffsetx>-8</textoffsetx>
        <textoffsety>0</textoffsety>
        <aligny>-</aligny>
    </default>
    <default type="spincontrolex">
        <posx>795</posx>
        <posy>240</posy>
        <width>450</width>
        <height>63</height>
        <spinwidth>60</spinwidth>
        <spinheight>92</spinheight>
        <texturefocus>views/tripanel/listselect_fo.png</texturefocus>
        <texturenofocus>-</texturenofocus>
        <textureup>scroll-up-2.png</textureup>
        <texturedown>scroll-down-2.png</texturedown>
        <textureupfocus>scroll-up-focus-2.png</textureupfocus>
        <texturedownfocus>scroll-down-focus-2.png</texturedownfocus>
