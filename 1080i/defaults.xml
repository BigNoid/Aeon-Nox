<?xml version="1.0" encoding="utf-8"?>
<includes>
    <default type="image">
        <posx>0</posx>
        <posy>0</posy>
        <texture>-</texture>
        <colorkey>0</colorkey>
    </default>
    <default type="multiimage">
        <include>FullscreenDimensions</include>
        <timeperimage>7000</timeperimage>
        <randomize>true</randomize>
        <fadetime>900</fadetime>
        <aspectratio align="center">scale</aspectratio>
        <include condition="![Window.IsActive(1115) | Window.IsActive(weather)]">Animation_WindowFade250</include>
    </default>
    <default type="label">
        <posx>120</posx>
        <posy>90</posy>
        <label>-</label>
        <font>font13</font>
        <textcolor>white</textcolor>
        <selectedcolor>$VAR[ColorDiffuseVar]</selectedcolor>
        <scrollspeed>20</scrollspeed>
    </default>
    <default type="fadelabel">
        <posx>120</posx>
        <posy>90</posy>
        <font>font13</font>
        <textcolor>white</textcolor>
        <pauseatend>3000</pauseatend>
        <scrollout>false</scrollout>
    </default>
    <default type="button">
        <posx>450</posx>
        <posy>300</posy>
        <width>450</width>
        <height>63</height>
        <texturefocus border="5">views/tripanel/listselect_fo.png</texturefocus>
        <texturenofocus border="5">button-nofocus.png</texturenofocus>
        <font>Font_OSDSeek2</font>
        <pulseonselect>yes</pulseonselect>
        <textcolor>white</textcolor>
        <disabledcolor>grey3</disabledcolor>
        <focusedcolor>white</focusedcolor>
        <textoffsetx>10</textoffsetx>
        <aligny>center</aligny>
    </default>
    <default type="togglebutton">
        <posx>450</posx>
        <posy>300</posy>
        <width>450</width>
        <height>63</height>
        <pulseonselect>yes</pulseonselect>
        <texturefocus border="5">views/tripanel/listselect_fo.png</texturefocus>
        <texturenofocus border="5">button-nofocus.png</texturenofocus>
        <alttexturefocus border="5">views/tripanel/listselect_fo.png</alttexturefocus>
        <alttexturenofocus border="5">dialogs/context_nofocus.png</alttexturenofocus>
        <font>Font_OSDSeek2</font>
        <textcolor>white</textcolor>
        <disabledcolor>grey3</disabledcolor>
        <focusedcolor>white</focusedcolor>
        <textoffsetx>10</textoffsetx>
        <aligny>center</aligny>
    </default>
    <default type="textbox">
        <posx>330</posx>
        <posy>330</posy>
        <width>615</width>
        <height>322</height>
        <spinwidth>18</spinwidth>
        <spinheight>16</spinheight>
        <spinposx>410</spinposx>
        <spinposy>305</spinposy>
