<?xml version="1.0" encoding="utf-8"?>
<window type="dialog" id="1156">
    <defaultcontrol always="true">1000</defaultcontrol>
    <onunload condition="Window.IsActive(1124) + [Skin.HasSetting(mainmenuicons) | !IsEmpty(Window(1124).Property(ResetItem))] + Stringcompare(Container(9000).NumItems,21)">Control.Move(9000,9)</onunload>
    <onunload condition="Window.IsActive(1124) + [Skin.HasSetting(mainmenuicons) | !IsEmpty(Window(1124).Property(ResetItem))] + Stringcompare(Container(9000).NumItems,22)">Control.Move(9000,10)</onunload>
    <onunload condition="!Window.IsActive(1124) + System.IdleTime(30) + !Window.IsActive(1154) + !Window.IsActive(visualisation) + Player.HasAudio">ActivateWindow(1154)</onunload>
    <onunload condition="!Window.IsActive(1124) + System.IdleTime(30) + ![Control.HasFocus(5012) | Control.HasFocus(6012) | Control.HasFocus(7012) | Control.HasFocus(8012) | Control.HasFocus(9012)]">SetFocus(7012)</onunload>
    <onunload condition="!Window.IsActive(1124) + System.IdleTime(30)">Action(Right)</onunload>
    <onunload condition="!Window.IsActive(1124) + System.IdleTime(30)">Control.Move(5010,1)</onunload>
    <onunload condition="!Window.IsActive(1124) + System.IdleTime(30)">Control.Move(6010,1)</onunload>
    <onunload condition="!IsEmpty(Window(1124).Property(ResetItem))">SetFocus(9006)</onunload>
    <onunload>ClearProperty(ResetItem,1124)</onunload>
    <zorder>5</zorder>
    <coordinates>
        <posx>0</posx>
        <posy>0</posy>
        <system>1</system>
    </coordinates>
    <controls>
        <control type="button" id="1000">
            <include>HiddenObject</include>
            <onfocus condition="!Window.IsActive(1124)">Dialog.Close(1156)</onfocus>
        </control>
        <control type="group">
            <visible>Window.IsActive(1124)</visible>
            <animation type="WindowOpen" reversible="false">
                <effect type="zoom" start="80" end="100" center="960,540" tween="back" time="240" />
                <effect type="fade" end="100" time="240" />
            </animation>
            <animation type="WindowClose" reversible="false">
                <effect type="zoom" start="100" end="80" center="960,540" easing="in" tween="back" time="240" />
                <effect type="fade" start="100" end="0" time="240" />
            </animation>
            <animation type="Visible" reversible="false">
                <effect type="zoom" start="80" end="100" center="960,540" delay="160" tween="back" time="240" />
                <effect type="fade" delay="160" end="100" time="240" />
            </animation>
            <animation type="Hidden" reversible="false">
                <effect type="zoom" start="100" end="80" center="960,540" easing="in" tween="back" time="240" />
                <effect type="fade" start="100" end="0" time="240" />
            </animation>
            <include>DialogBackFade</include>
            <control type="image">
                <posx>923</posx>
                <posy>503</posy>
                <width>74</width>
                <height>74</height>
                <texture>loader.gif</texture>
                <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
                <animation effect="fade" start="70" time="1200" condition="true" pulse="true">Conditional</animation>
            </control>
        </control>
    </controls>
</window>