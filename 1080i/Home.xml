            <include>Animation_WindowFade200</include>
            <control type="image">
                <description>media info background image</description>
                <posx>0</posx>
                <posy>360</posy>
                <width>1920</width>
                <height>945</height>
                <texture>home/nowplaying_shadow.png</texture>
                <visible>[Control.HasFocus(8000) | Control.IsVisible(5000) | Substring(Control.GetLabel(4321),weather)] + !Skin.HasSetting(Nox.DisableWidgets)</visible>
                <include>Animation_VisibleChangeDelayed</include>
                <animation effect="slide" time="400" tween="cubic" easing="inout" end="0,-225" condition="Skin.HasSetting(Nox.lowermainmenu)">Conditional</animation>
            </control>
            <control type="image">
                <description>media info background image</description>
                <posx>0</posx>
                <posy>0</posy>
                <width>1920</width>
                <height>945</height>
                <texture flipy="true">home/nowplaying_shadow.png</texture>
                <include>Animation_VisibleChangeDelayed</include>
                <visible>[Player.HasAudio + !Skin.HasSetting(Nox.nohomepageMusicinfo) + [IsEmpty(Control.GetLabel(4325)) | Skin.HasSetting(Nox.DisableWidgets)]] | [Player.HasVideo + !Skin.HasSetting(Nox.homepageVideoinfo)] | [[Control.HasFocus(7000) | Control.IsVisible(6000) | Substring(Control.GetLabel(4325),weather)] + !Skin.HasSetting(Nox.DisableWidgets)]</visible>
                <animation effect="slide" time="400" tween="cubic" easing="inout" end="0,-225" condition="Skin.HasSetting(Nox.lowermainmenu)">Conditional</animation>
            </control>
        </control>
