<?xml version="1.0" encoding="utf-8"?>
<window>
    <id>123</id>
    <defaultcontrol>2</defaultcontrol>
    <coordinates>
        <system>1</system>
        <posx>0</posx>
        <posy>0</posy>
    </coordinates>
    <animation type="WindowOpen" reversible="false">
        <effect type="zoom" start="80" end="100" center="960,540" easing="out" tween="back" time="300" />
        <effect type="fade" start="0" end="100" time="300" />
    </animation>
    <animation type="WindowClose" reversible="false">
        <effect type="zoom" start="100" end="80" center="960,540" easing="in" tween="back" time="300" />
        <effect type="fade" start="100" end="0" time="300" />
    </animation>
    <control type="image">
        <description>Panel</description>
        <posx>300</posx>
        <posy>180</posy>
        <width>1322</width>
        <height>722</height>
        <texture>osd/osd_settings_back_glow.png</texture>
        <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
        <visible>!Skin.HasSetting(Nox.DisableGlowbar)</visible>
        <include>PanelGlowFade</include>
        <include>Animation_VisibleChange400</include>
        <include>PanelGlowFade</include>
    </control>
    <controls>
        <control type="image">
            <description>Panel</description>
            <posx>300</posx>
            <posy>180</posy>
            <colordiffuse>$VAR[DialogColorVar]</colordiffuse>
            <width>1322</width>
            <height>722</height>
            <texture>osd/osd_settings_back.png</texture>
        </control>
        <control type="label" id="3">
            <description>Header Label</description>
            <posx>420</posx>
            <posy>225</posy>
            <width>600</width>
            <height>60</height>
            <textcolor>white</textcolor>
            <font>Font_Info</font>
            <label>$LOCALIZE[13406]</label>
            <align>left</align>
            <aligny>center</aligny>
        </control>
        <control type="label" id="4">
            <description>No Presets Label</description>
            <posx>375</posx>
            <posy>112</posy>
            <width>1172</width>
            <height>600</height>
            <align>center</align>
            <aligny>center</aligny>
            <label>13390</label>
            <font>Font_Info</font>
        </control>
        <control type="list" id="2">
            <posx>375</posx>
            <posy>292</posy>
            <width>1172</width>
            <height>540</height>
            <onleft>60</onleft>
            <onright>60</onright>
            <onup>2</onup>
            <ondown>2</ondown>
            <scrolltime>200</scrolltime>
            <pagecontrol>60</pagecontrol>
            <itemlayout height="60">
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>1172</width>
                    <height>60</height>
                    <texture>osd/osd_settings_buttonnf.png</texture>
                </control>
                <control type="label">
                    <posx>45</posx>
                    <posy>0</posy>
                    <width>1082</width>
                    <height>60</height>
                    <textcolor>FF929191</textcolor>
                    <aligny>center</aligny>
                    <font>Font_Info</font>
                    <info>ListItem.Label</info>
                </control>
            </itemlayout>
            <focusedlayout height="60">
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>1172</width>
                    <height>60</height>
                    <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
                    <texture>views/tripanel/listselect_fo.png</texture>
                </control>
                <control type="label">
                    <posx>45</posx>
                    <posy>0</posy>
                    <width>1082</width>
                    <height>60</height>
                    <textcolor>white</textcolor>
                    <aligny>center</aligny>
                    <font>Font_Info</font>
                    <info>ListItem.Label</info>
                </control>
            </focusedlayout>
        </control>
