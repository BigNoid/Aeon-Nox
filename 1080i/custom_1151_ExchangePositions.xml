                <scrolltime>0</scrolltime>
                <orientation>vertical</orientation>
                <onright>20</onright>
                <itemlayout height="40" width="300">
                    <control type="image">
                        <width>300</width>
                        <height>40</height>
                        <texture>button-nofocus.png</texture>
                        <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>150</posx>
                        <width>300</width>
                        <height>40</height>
                        <textoffsetx>10</textoffsetx>
                        <align>center</align>
                        <label>$INFO[ListItem.Label]</label>
                        <font>Font_Reg26_Caps</font>
                    </control>
                </itemlayout>
                <focusedlayout height="40" width="300">
                    <control type="group">
                        <control type="image">
                            <width>300</width>
                            <height>40</height>
                            <texture border="8">views/tripanel/listselect_fo.png</texture>
                            <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
                        </control>
                        <control type="label">
                            <posx>150</posx>
                            <width>300</width>
                            <height>40</height>
                            <textoffsetx>10</textoffsetx>
                            <align>center</align>
                            <label>$INFO[ListItem.Label]</label>
                            <font>Font_Reg26_Caps</font>
                            <scroll>true</scroll>
                        </control>
                    </control>
                </focusedlayout>
                <content>
                    <include>CustomizerMenuItems</include>
                </content>
            </control>
